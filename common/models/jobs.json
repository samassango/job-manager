{
  "name": "Jobs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "skills": {
      "type": ["string"],
      "required": true
    },
    "budget": {
      "type": "number",
      "required": true
    },
    "verified": {
      "type": "boolean",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    },
    "lastUpdated": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "status": {
      "type": "belongsTo",
      "model": "status",
      "foreignKey": "status",
      "options": {
        "nestRemoting": true
      }
    },
    "User": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "author",
      "options": {
        "nestRemoting": true
      }
    },
    "tanent": {
      "type": "belongsTo",
      "model": "tanent",
      "foreignKey": "tanent_id",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
